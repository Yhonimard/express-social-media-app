{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run migrate:dev && npm run generate && dotenv -e .env -- babel src -d build",
    "dev": "cross-env NODE_ENV=dev dotenv -e .env.dev -- nodemon --exec babel-node src/index",
    "start": "dotenv -e .env babel-node src/index.js",
    "start:container" : "npm run build && npm run build && node build/index.js",
    "seed:dev": "dotenv -e .env.dev babel-node src/prisma/seed.js",
    "seed:stg": "dotenv -e .env babel-node src/prisma/seed.js",
    "migrate:prod": "dotenv -e .env -- npx prisma migrate deploy --schema ./src/prisma/schema.prisma",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev --schema ./src/prisma/schema.prisma",
    "push:dev": "dotenv -e .env.dev -- npx prisma db push --schema ./src/prisma/schema.prisma",
    "push:prod": "dotenv -e .env -- npx prisma db push --schema ./src/prisma/schema.prisma",
    "generate": "npx prisma generate --schema ./src/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@prisma/client": "^5.3.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status": "^1.7.0",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.22.20",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.3.1"
  }
}
